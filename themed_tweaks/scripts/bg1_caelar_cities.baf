/////////////////////////////////////
// Caelar shows up in cities/towns //
/////////////////////////////////////

IF
	GlobalTimerExpired("#L_BG1Caelar_Timer","GLOBAL")
	GlobalGT("#L_BG1Caelar_Visit","GLOBAL",0)
THEN
	RESPONSE #100
		CreateCreatureObjectOffset("BDCAELAR",Player1,[50.50])
		SmallWait(10)
		ActionOverride("BDCAELAR",StartDialogOverride("#LCaelar",Player1))
END
